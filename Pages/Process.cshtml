@page
@model hellorazor.Pages.ProcessModel
@{
    // c# island
    string[]? people = ViewData["people"] as string[];
}


<div>
    <h2>Process Page</h2>
    <h4>@ViewData["Message"]</h4>
    <ul>
        @foreach (var person in people!)
        {
            <li>@person</li>
        }
    </ul>
</div>